{
    "name": "strawberry-graphql",
    "version": "1.0.0",
    "author": "Patrick Arminio <patrick.arminio@@gmail.com>",
    "engines": {
        "node": "14.x",
        "npm": "7.x"
    },
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "prebuild": "npm run codegen",
        "build": "next build",
        "develop": "npm run dev",
        "dev": "next dev",
        "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
        "start": "next start",
        "format": "prettier --write \"{,!(.next|node_modules|public|coverage|.cache)/**/}*.{js,jsx,ts,tsx,json}\"",
        "test": "echo \"Error: no test specified\" && exit 1",
        "type-check": "tsc --noEmit",
        "type-check:watch": "tsc --watch --noEmit",
        "codegen": "graphql-codegen --config codegen.yml",
        "codegen:watch": "npm run codegen -- --watch",
        "check-updates": "npm-check -u",
        "lint": "next lint"
    },
    "keywords": [
        "strawberry-graphql",
        "strawberry",
        "graphql",
        "next.js",
        "typescript"
    ],
    "dependencies": {
        "@docsearch/js": "3.0.0",
        "@docsearch/react": "3.0.0",
        "@hashicorp/remark-plugins": "3.3.1",
        "@heroicons/react": "1.0.6",
        "@octokit/core": "3.6.0",
        "@octokit/plugin-paginate-rest": "2.17.0",
        "@tailwindcss/aspect-ratio": "0.4.0",
        "classnames": "2.3.1",
        "github-slugger": "1.4.0",
        "gray-matter": "4.0.3",
        "hast-util-to-html": "8.0.3",
        "hast-util-to-string": "2.0.0",
        "jsonp": "0.2.1",
        "marked": "3.0.8",
        "mdast-util-heading-range": "3.1.0",
        "next": "12.1.0",
        "next-mdx-remote": "4.0.2",
        "npm-check": "5.9.2",
        "nprogress": "0.2.0",
        "parse-numeric-range": "1.3.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-instantsearch-dom": "6.22.0",
        "react-refractor": "2.1.6",
        "refractor": "4.5.0",
        "rehype-parse": "8.0.4",
        "to-querystring": "1.1.1",
        "typescript": "4.6.2",
        "unified": "10.1.2",
        "unist-util-visit": "4.1.0",
        "unist-util-visit-parents": "5.1.0"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "2.6.2",
        "@graphql-codegen/typescript": "2.4.7",
        "@graphql-codegen/typescript-operations": "2.3.4",
        "@octokit/graphql-schema": "10.73.0",
        "@trivago/prettier-plugin-sort-imports": "3.2.0",
        "@types/github-slugger": "1.3.0",
        "@types/jsonp": "0.2.1",
        "@types/marked": "3.0.3",
        "@types/node": "17.0.21",
        "@types/nprogress": "0.2.0",
        "@types/react": "17.0.40",
        "@types/react-dom": "17.0.13",
        "@types/refractor": "3.0.2",
        "@typescript-eslint/eslint-plugin": "5.15.0",
        "autoprefixer": "10.4.4",
        "eslint": "8.11.0",
        "eslint-config-next": "12.1.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-prettier": "4.0.0",
        "graphql": "15.8.0",
        "prettier": "2.6.0",
        "tailwindcss": "3.0.23"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/strawberry-graphql/strawberry.rocks"
    }
}
