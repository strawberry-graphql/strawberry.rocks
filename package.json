{
    "name": "strawberry-graphql",
    "version": "1.0.0",
    "author": "Patrick Arminio <patrick.arminio@@gmail.com>",
    "engines": {
        "node": "14.x",
        "npm": "7.x"
    },
    "scripts": {
        "prebuild": "npm run codegen && npm run type-check",
        "build": "next build",
        "develop": "npm run dev",
        "dev": "next dev",
        "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
        "start": "next start",
        "update-algolia": "ts-node --project scripts/tsconfig.json -r dotenv/config scripts/update-algolia.ts dotenv_config_path=.env.local",
        "format": "prettier --write \"{,!(.next|node_modules|public|coverage|.cache)/**/}*.{js,jsx,ts,tsx,json}\"",
        "test": "echo \"Error: no test specified\" && exit 1",
        "type-check": "tsc --noEmit",
        "type-check:watch": "tsc --watch --noEmit",
        "codegen": "graphql-codegen --config codegen.yml",
        "codegen:watch": "npm run codegen -- --watch",
        "lint": "eslint --cache --ext .js,.ts,.tsx ."
    },
    "keywords": [
        "strawberry-graphql",
        "strawberry",
        "graphql",
        "next.js",
        "typescript"
    ],
    "dependencies": {
        "@docsearch/react": "^3.0.0-alpha.40",
        "@hashicorp/remark-plugins": "^3.1.1",
        "@jsdevtools/rehype-toc": "^3.0.2",
        "@mdx-js/loader": "^1.6.22",
        "@mdx-js/mdx": "^1.6.22",
        "@mdx-js/react": "^1.6.22",
        "@next/mdx": "^11.1.2",
        "@octokit/core": "^3.4.0",
        "@octokit/plugin-paginate-rest": "^2.16.3",
        "@tailwindcss/aspect-ratio": "^0.2.0",
        "algoliasearch": "^4.10.5",
        "classnames": "^2.3.1",
        "dotenv": "^10.0.0",
        "github-slugger": "^1.4.0",
        "gray-matter": "^4.0.3",
        "hast-util-to-html": "^8.0.2",
        "hast-util-to-string": "^2.0.0",
        "jsonp": "^0.2.1",
        "marked": "^3.0.4",
        "mousetrap": "^1.6.5",
        "next": "^11.1.1",
        "next-mdx-remote": "^3.0.4",
        "nprogress": "^0.2.0",
        "parse-numeric-range": "^1.3.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-helmet": "^6.1.0",
        "react-instantsearch-dom": "^6.10.3",
        "react-use": "^17.3.1",
        "refractor": "^4.2.1",
        "rehype-parse": "^8.0.3",
        "react-refractor": "^2.1.5",
        "to-querystring": "^1.1.1",
        "ts-node": "^10.2.1",
        "typescript": "^4.4.3",
        "unified": "^10.1.0",
        "unist-util-is": "^5.1.1",
        "unist-util-map": "^3.0.0",
        "unist-util-reduce": "^0.2.2",
        "unist-util-visit": "^4.1.0",
        "unist-util-visit-parents": "^5.1.0"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^2.2.0",
        "@graphql-codegen/typescript": "^2.2.2",
        "@graphql-codegen/typescript-operations": "^1.17.16",
        "@octokit/graphql-schema": "^10.72.0",
        "@tailwindcss/jit": "^0.1.18",
        "@trivago/prettier-plugin-sort-imports": "^2.0.4",
        "@types/classnames": "^2.3.1",
        "@types/github-slugger": "^1.3.0",
        "@types/jsonp": "^0.2.1",
        "@types/marked": "^3.0.0",
        "@types/mdx-js__react": "^1.5.3",
        "@types/mousetrap": "^1.6.8",
        "@types/node": "^16.10.1",
        "@types/nprogress": "^0.2.0",
        "@types/react": "^17.0.24",
        "@types/react-dom": "^17.0.5",
        "@types/react-helmet": "^6.1.1",
        "@types/refractor": "^3.0.0",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "autoprefixer": "^10.3.6",
        "eslint": "^7.21.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.26.0",
        "graphql": "^15.6.0",
        "postcss": "^8.3.8",
        "prettier": "^2.4.1",
        "tailwindcss": "^2.2.7"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/strawberry-graphql/strawberry.rocks"
    }
